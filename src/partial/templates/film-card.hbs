{{#each this}}
  <li class='film-list__item'>
    <div class='film-card'>

      <a class='film-card__link' href={{}}>
        <img 
          class='film-card__image' 
          src={{}} 
          alt={{}}
        />
      </a>

      <div class="film-card__body">
        <h3 class="film-card__title"></h3>
        <p class="film-card__data">{{}}<span></span>{{}}</p>
      </div>

    </div>
  </li>
{{/each}}